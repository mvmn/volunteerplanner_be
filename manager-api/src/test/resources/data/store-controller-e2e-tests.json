  {
    "name": "POST: search by name",
    "given": {
      "auth": {
        "principal": "222222222222",
        "password": "operator"
      },
      "request": {
        "endpoint": "/api/v1/stores/search",
        "method": "POST",
        "body": {
          "page": 1,
          "pageSize": 10,
          "filter": {
            "type": "operator",
            "operator": "and",
            "operands": [
              {
                "type": "text",
                "field": "name",
                "value": "store"
              },
              {
                "type": "number",
                "field": "region.id",
                "value": 1
              },
              {
                "type": "number",
                "field": "city.id",
                "value": 15
              },
              {
                "type": "bool",
                "field": "confidential",
                "value": true
              }
            ]
          },
          "sort": {
            "field": "city.name",
            "order": "asc"
          }
        }
      }
    },
    "expected": {
      "status": 200,
      "body": {
        "page": 1,
        "pageSize": 10,
        "totalCount": 1,
        "items": [
          {
            "id": 3,
            "name": "private store",
            "city": {
              "id": 15,
              "region": {
                "id": 1,
                "name": "Вінницька"
              },
              "name": "Гнівань"
            },
            "address": "private address",
            "confidential": true,
            "note": "private note"
          }
        ]
      }
    }
  },
